<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.exercise.androidfinalproject.ChangeDataFragment"
        android:label="@string/title_home"
        tools:layout="@layout/changedata_layout" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.exercise.androidfinalproject.ProfileFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/profile_layout" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.exercise.androidfinalproject.SensorsDataFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/sensorsdata_layout" />
</navigation>